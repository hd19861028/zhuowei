	/** elasticsearch的定时备份清理工作 **/
	"esClear": {
		"repository": "aliyun-emr",
		/** 备份的配置，第一项不可以有任何修改 **/
		"bk": [{
			"index": "*",
			"title": "snapshot"
		}, {
			"interval": "33 00 00 01 * *",
			"index": ["schedule", ".kibana"],
			"title": "snapshot_specify"
		}],
		/** 定时清理的配置 **/
		"del": [{
			/** 是否启动 **/
			"enable": false,
			/** 超时（秒） **/
			"timeout": 30,
			/** 清理的起始时间：从当前时间往前推多少天 **/
			"lessDay": 30,
			/** 清理的结束时间：从起始时间开始，往前推多少天 **/
			"areaDay": 7,
			/** 索引数组，不支持模糊匹配 **/
			"index": ["device_command_v1", "device_notification_v1", "nginx-access-weixin", "nginx-access-cms", "nginx-access-devicehive", "nginx-access-vrstore", "nginx-access-vrstore_manage", "tomcat-catalina-vrstore", "tomcat-catalina-devicehive", "webapps-adas", "webapps-cms", "webapps-dchub", "webapps-devicehive", "webapps-eventengine", "webapps-ota", "webapps-vrstore", "webapps-weixin"],
			"title": "es-clear",
			"interval": "35 10 00 * * *"
		}]
	},
